  npatches = 75
  do i = 1,npatches
     norders(i) = 4
  end do
  do i = 1,npatches+1
     ixyzs(i) = 1 + 25*i
  end do
  do i = 1,npatches
     iptype(i) = 11
  end do
  npts = 1875
  

  